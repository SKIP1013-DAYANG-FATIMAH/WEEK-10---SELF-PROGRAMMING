package selfProgramming;

public class Exercise {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i=2;
		int j=3;
		int k;
		
		while (i<10) {
			System.out.println("i = " + i);
				i=i+1;
			}
		
		do { 
			System.out.println("j = " + j);
			j=j+1;
			
		} while (j<15);
		
		for (k=1 ; k<5 ; k=k+1) {
			System.out.println("k = " + k);
		}
		
	}

}
